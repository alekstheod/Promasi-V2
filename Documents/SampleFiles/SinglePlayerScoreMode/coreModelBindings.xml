<CoreModelBindings>
	<OutputVariables>
		<OutputVariable>
			<sdObjectKey>projectFinished</sdObjectKey>
			<modelXPath>currentProject/percentageCompleted
			</modelXPath>
		</OutputVariable>
	</OutputVariables>
	<ExternalEquations>
		<ExternalEquation>
			<sdObjectKey>sumDeveloper</sdObjectKey>
			<modelXPath>currentProject/tasks[name='Develop']/currentSchedule/team/assignedEmployees[not(developer &lt;= preceding-sibling::assignedEmployees/developer) and not(developer &lt;= following-sibling::assignedEmployees/developer)]/developer</modelXPath>
		</ExternalEquation>
	</ExternalEquations>
	<Events>
		<Event>
			<sdObjectKey>productivity</sdObjectKey>
			<eventName>lowProductivity</eventName>
			<action>
				<class>org.promasi.shell.modelactions.BossMailModelAction</class>
				<param name="message">The productivity of your developers is low!</param>
			</action>
		</Event>
	</Events>
</CoreModelBindings>